<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mig.audit">
	<sql id="col">
		<![CDATA[
		SRC_ID   ,
		TAG_ID   ,
		ACTION   ,
		RESULT   , 
		MSG      ,
		CREATOR  ,
		TIME	 
		]]>
	</sql>
	<sql id="val">
		<![CDATA[
		#{srcId				},
		#{tagId				},	
		#{action			},
		#{result			},
		#{msg				},
		'SYSMIG'			 ,	
		CURRENT_TIMESTAMP
		]]>
	</sql>
	<sql id="set">
		<![CDATA[
		]]>
	</sql>
</mapper>